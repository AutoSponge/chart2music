{
    "name": "chart2music",
    "version": "0.1.0",
    "main": "dist/index.js",
    "description": "Turns charts into music so the blind can hear data",
    "keywords": [
        "accessibility",
        "a11y",
        "chart",
        "graph",
        "audio",
        "audio graph",
        "data visualization",
        "dataviz",
        "screen reader"
    ],
    "homepage": "https://github.com/julianna-ciq/chart2music#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/julianna-ciq/chart2music.git"
    },
    "scripts": {
        "build": "tsc",
        "clean": "rimraf ./dist",
        "start": "npm run build --watch && http-server .",
        "lint": "eslint src/ --ext .ts --fix",
        "format": "prettier --config .prettierrc.json src/**/*.ts --write",
        "prepare": "husky install"
    },
    "license": "MIT",
    "devDependencies": {
        "@typescript-eslint/parser": "^5.10.2",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jsdoc": "^39.3.2",
        "http-server": "^14.1.1",
        "husky": "^6.0.0",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.7.3"
    }
}
